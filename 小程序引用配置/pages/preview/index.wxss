@import "../../style/reset.wxss";
/**index.wxss**/
.home_ban{
	padding:60rpx 90rpx 0;
}
.home_top{
	background:#fff;
	box-sizing:0 0 20rpx rgba(0,0,0,0.3);
}
.home_top_bg{
	background:#0096e0;
	text-align:center;
	position:relative;
	height:420rpx;
}
.home_top_bg_a{
	background:#b0815e;
}
.home_top_hpone{
    margin:120rpx 0 30rpx;
	width:260rpx;
	height:260rpx;
	border-radius:50%;
	-webkit-border-radius:50%;
	overflow:hidden;

}
.home_top_img{
	width:100%;
	height:100%;
	border-radius:50%;
	-webkit-border-radius:50%;
}
.home_top_title{
	position:absolute;
	bottom:8rpx;
	right:30rpx;
	font-size:24rpx;
	color:#fff;
}
.home_cont{
	text-align:center;
	display:block;
	height:420rpx;
	
}

.home_cont_name{
	font-size:90rpx;
	color:#3f3a39;
	display:block;
	padding:86rpx 0 30rpx;
}
.home_cont_logo{
	height:90rpx;
	padding:30rpx 0;
	width:180rpx;
}


.preview_bottom{
	position:absolute;
    bottom:36rpx;
    lefet:0;
    width:100%;
}
.preview_btn{
    margin:0 40rpx;
	display: block;
}
.preview_btn_item{
	display:block;
	width:46%;
	border:1rpx solid #0096e0;
	border-radius:	49rpx;
	-webkit-border-radius:	49rpx;
	background:#0096e0;
	font-size:32rpx;
	color:#fff;
	text-align:center;
	line-height:98rpx;
}
.preview_btn_item:nth-of-type(1){
	background:#fff;
	color:#0096e0;
}

