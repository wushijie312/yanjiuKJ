@import "../../style/reset.wxss";
/**index.wxss**/
.home_ban{
	padding:60rpx 90rpx 0;
	overflow:hidden;
}
.home_top{
	background:#fff;
	box-sizing:0 0 20rpx rgba(0,0,0,0.3);
}
.home_top_bg{
	background:#0096e0;
	text-align:center;
	position:relative;
	height:420rpx;
}
.home_top_bg_a{
	background:#b0815e;
}
.home_top_hpone{
    margin:120rpx 0 30rpx;
	width:260rpx;
	height:260rpx;
	border-radius:50%;
	-webkit-border-radius:50%;
	overflow:hidden;

}
.home_top_img{
	width:100%;
	height:100%;
	border-radius:50%;
	-webkit-border-radius:50%;
}
.home_top_title{
	position:absolute;
	bottom:8rpx;
	right:30rpx;
	font-size:24rpx;
	color:#fff;
}
.home_cont{
	text-align:center;
	display:block;
	height:420rpx;
	
}
.home_cont_a{
	background:#fff url('http://static.98ep.com/Img/yiXinCard/mould_b_line.png')  center top no-repeat;
	background-size:100%;
}
.home_cont_b{
	background:#fff url('http://static.98ep.com/Img/yiXinCard/mould_c_line.png')  center top no-repeat;
	background-size:100%;
}
.home_cont_name{
	font-size:90rpx;
	color:#3f3a39;
	display:block;
	padding:86rpx 0 30rpx;
}
.home_cont_logo{
	height:90rpx;
	padding:30rpx 0;
	width:180rpx;
}
.home_btn_box{
	padding:36rpx 0;
	display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
}
.home_btn_item{
	display:block;
	text-align:center;
	width:15%;
	font-size:28rpx;
	color:#999;
	padding:25rpx 18rpx;
	border:1rpx solid #999;
	border-radius:10rpx;
	-webkit-border-radius:10rpx;
	background:#fff;
}
.home_btn{
	border-radius:	49rpx;
	-webkit-border-radius:	49rpx;
	background:#0096e0;
	font-size:32rpx;
	color:#fff;
	text-align:center;
	line-height:98rpx;
	margin:0 40rpx;
}
.hight{
	color:#0096e0;
	border:1px solid #0096e0;
}

